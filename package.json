{
  "name": "netlifyfunctions",
  "version": "1.0.0",
  "description": "My world-class Nuxt.js project",
  "author": "porith",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "generate": "nuxt generate",
    "bootstrap": "node ./bootstrap-fauna-database.js",
    "start": "npm-run-all --parallel start:app start:server",
    "start:app": "nuxt start",
    "start:server": "netlify-lambda serve lambdas",
    "prebuild": "echo 'setup faunaDB' && npm run bootstrap",
    "build": "npm-run-all --parallel build:**",
    "build:app": "nuxt generate",
    "build:functions": "netlify-lambda build lambdas"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/proxy": "^1.3.3",
    "@nuxtjs/pwa": "^2.6.0",
    "axios": "^0.19.0",
    "cross-env": "^5.2.0",
    "faunadb": "^2.6.1",
    "nuxt": "^2.3.4"
  },
  "devDependencies": {
    "netlify-lambda": "^1.4.13",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
